{
  "name": "@bot-machine/telegram-client",
  "version": "1.0.3",
  "description": "A TypeScript HTTP client for the Telegram Bot API built with Bun",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "bun run index.ts",
    "test": "bun test",
    "example": "bun run example.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "telegram",
    "bot",
    "api",
    "client",
    "typescript",
    "bun"
  ],
  "author": "Computer Master",
  "license": "MIT",
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaizin-digital/telegram-client.git"
  },
  "bugs": {
    "url": "https://github.com/kaizin-digital/telegram-client/issues"
  },
  "homepage": "https://github.com/kaizin-digital/telegram-client#readme"
}
